<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>
            Index.jsp
        </welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.whats2000.controller.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/submitLoginInfo</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CargoTableServlet</servlet-name>
        <servlet-class>com.whats2000.controller.CargoTableServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CargoTableServlet</servlet-name>
        <url-pattern>/CargoTableServlet</url-pattern>
    </servlet-mapping>
</web-app>
